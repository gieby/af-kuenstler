<div id="kuenstler-inhalt">
		<?php foreach($this->blocks as $block): ?>
			<h2><?= $block['title']; ?></h2>
			<table>
				<?php foreach($block['entries'] as $entry): ?>
					<?php if($entry['dropFirstColumn']): ?>
					<tr>
							<td><?= $entry['text'] ?></td>
						</tr>
					<?php else: ?>
						<tr>
							<td class="jahr"><?= $entry['date'] ?></td>
							<td><?= $entry['text'] ?></td>
						</tr>
					<?php endif; ?>
				<?php endforeach; ?>
			</table>
		<?php endforeach; ?>
</div>